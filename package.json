{
  "name": "poke-api",
  "version": "1.0.0",
  "description": "Poke-API",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "lint": "npx eslint '**/*.ts'",
    "compile": "ts-node build.ts && tsc -p ./config/tsconfig-build.json",
    "dev": "NODE_ENV=local nodemon server/index.ts | pino-pretty",
    "dev-qa": "NODE_ENV=local nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "dev:debug-qa": "NODE_ENV=local nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "test": "TZ=UTC NOLOG=false env-cmd -f .env mocha 'test/**/*.ts' --reporter=nyan --exit",
    "test:debug": "env-cmd -f .env mocha --inspect-brk 'test/**/*.ts' --exit",
    "test:coverage": "TZ=UTC NOLOG=true env-cmd -f .env nyc --reporter=lcov mocha 'test/**/*.ts' --reporter mocha-junit-reporter --reporter-options mochaFile=./coverage/mocha/test-results.xml --exit"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "camelcase-keys": "^6.0.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-correlation-id": "^1.3.0",
    "express-mung": "^0.5.1",
    "express-pino-logger": "^4.0.0",
    "express-validator": "^6.2.0",
    "http-status-codes": "^1.4.0",
    "lodash": "^4.17.19",
    "mongoose": "^5.10.6",
    "pino": "^5.13.1",
    "snakecase-keys": "^3.1.0",
    "ts-stopwatch": "^0.0.4",
    "uuid": "^3.3.3",
    "yarn": "^1.22.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.11.1",
    "@types/pino": "^5.8.8",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dot-prop": "^5.2.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "lint-staged": "^10.1.7",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "nock": "^12.0.3",
    "nodemon": "^1.19.1",
    "nyc": "^15.0.0",
    "pino-pretty": "^3.2.0",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "source-map-support": "^0.5.13",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.9.2",
    "yaml-validator": "^2.2.0"
  },
  "lint-staged": {
    "*.(ts)": [
      "eslint **/*.ts --max-warnings 3 --fix"
    ]
  },
  "author": "Dennys Mantovani",
  "license": "UNLICENSED"
}
